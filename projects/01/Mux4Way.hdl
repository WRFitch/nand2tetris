/**
 * 4-way multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way {
    IN a, b, c, d, sel;
    OUT out;

    PARTS:
    Mux(a=a, b=b, sel=sel[1], out=m1);
    Mux(a=c, b=d, sel=sel[1], out=m2);
    
    Mux(a=m1, b=m2, sel=sel[0], out=out);
}
